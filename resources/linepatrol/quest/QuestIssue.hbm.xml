<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.linepatrol.quest.module.QuestIssue"  table="QUEST_ISSUE">
        <id name="id" type="string" unsaved-value="null" >
            <column name="id" sql-type="VARCHAR(12)" not-null="true"/>  
            <generator class="com.cabletech.commons.hb.StrSequenceGenerator">
               <param name="sequence">SEQ_QUEST_ISSUE_ID</param>
               <param name="length">12</param>
            </generator>
        </id>
        <property name="name" column="QUESTIONNAIRE_NAME"/>
        <property name="type" column="QUESTIONNAIRE_TYPE"/>
        <property name="remark" column="REMARK"/>
        <property name="state" column="STATE"/>
        <property name="creator" column="CREATOR"/>
        <property name="createDate" column="CREATE_DATE"/>
    </class>
</hibernate-mapping>