<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.watchinfo.domainobjects.Watch"  table="watchinfo">
        <!-- A 32 hex character is our surrogate key. It's automatically
            generated by Hibernate with the UUID pattern. -->
		<id name="placeID" type="string" unsaved-value="null" >
            <column name="PlaceID" sql-type="char(32)" not-null="true"/>  
            <generator class="assigned"/>
        </id>
        <property name="placeName"/>
        <property name="principal"/>
        <property name="terminalID"/>
        <property name="beginDate"/>
        <property name="endDate"/>
        <property name="patrolTime"/>
        <property name="orderlyBeginTime"/>
        <property name="orderlyEndTime"/>
        <property name="orderlyCyc"/>
        <property name="error"/>
        <property name="lid"/>
        <property name="regionID"/>
        <property name="startpointid"/>
		<property name="endpointid"/>
        <property name="watchwidth"/>
		<property name="gpscoordinate"/>

		<property name="contractorid"/>
		<property name="innerregion"/>
		<property name="watchplace"/>
		<property name="placetype"/>
		<property name="dangerlevel"/>
		<property name="watchreason"/>
		<property name="endwatchinfo"/>
		<property name="involvedlinenumber"/>
		<property name="ifcheckintime"/>
		<property name="checkresult"/>
		<property name="dealstatus"/>


    </class>
</hibernate-mapping>
