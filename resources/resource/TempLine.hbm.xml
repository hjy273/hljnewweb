<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.lineinfo.domainobjects.TempLine"  table="templineinfo">
        <id name="pointid" type="string" unsaved-value="null" >
            <column name="pointid" sql-type="char(32)" not-null="true"/>
            <generator class="assigned"/>
        </id>
        <property name="startpointgps"/>
        <property name="endpointgps"/>
        <property name="regionid"/>

    </class>
</hibernate-mapping>

