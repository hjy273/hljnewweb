<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.planinfo.domainobjects.YearMonthPlan"  table="yearmonthplan">
        <id name="id" type="string" unsaved-value="null" >
            <column name="id" sql-type="char(32)" not-null="true"/>
            <generator class="assigned"/>
        </id>
        <property name="planname"/>
        <property name="year"/>
        <property name="month"/>
        <property name="plantype"/>
        <property name="ifinnercheck"/>

<property name="approver"/>
<property name="approvedate"/>
<property name="remark"/>
<property name="status"/>
<property name="regionid"/>
<property name="creator"/>
<property name="createdate"/>
<property name="deptid"/>
<property name="createuserid"/>
    </class>
</hibernate-mapping>

