<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.baseinfo.domainobjects.Region"  table="Region">
        <id name="regionID" type="string" unsaved-value="null" >
            <column name="RegionID" sql-type="char(32)" not-null="true"/>
            <generator class="assigned"/>
        </id>
        <property name="regionName"/>
        <property name="regionDes"/>
        <property name="state"/>
		<property name="parentregionid"/>
    </class>
</hibernate-mapping>