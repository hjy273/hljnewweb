<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.menu.domainobjects.FirstMenu"  table="mainmodulemenu">
        <id name="id" type="string" column="id" >
            <generator class="sequence">
                <param name="sequence">SEQ_MAINMODULEMENU_ID</param>
        	</generator>
        </id>
        <property name="lablename" column="lablename" />
        <property name="imgurl" column="imgurl" />
        <property name="hrefurl" column="hrefurl" />
        <property name="remark" column="remark" />
       
    </class>
</hibernate-mapping>