<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.lineinfo.domainobjects.SmsSendLog"  table="localizermsglog">
        <id name="msg_id" type="string" unsaved-value="null" >
            <column name="msg_id" sql-type="char(32)" not-null="true"/>  
            <generator class="assigned"/>          
        </id>
        <property name="simid"/>
		<property name="sendtime"/>
        <property name="content"/>
		<property name="type"/> 
		<property name="isresponded"/>
		<property name="issent"/>
		<property name="senttime"/>
    </class>
</hibernate-mapping>