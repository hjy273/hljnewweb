<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.sparepartmanage.domainobjects.SparepartAuditing"  table="spare_part_auditingapply">
        <!-- A 32 hex character is our surrogate key. It's automatically
            generated by Hibernate with the UUID pattern. -->
        <id name="tid" type="string" unsaved-value="null" >
            <column name="tid" sql-type="char(32)" not-null="true"/>
            <generator class="assigned"/>
        </id>
        <property name="applyId" column="apply_id" type="string" />
        <property name="auditingResult" column="auditing_result" type="string" />
        <property name="auditingOpnion" column="auditing_opnion" type="string" />
        <property name="auditingDate" column="auditing_date" type="timestamp" />
        <property name="auditingPerson" column="auditing_person" type="string" />
        <property name="auditingRemark" column="auditing_remark" type="string" />
    </class>
</hibernate-mapping>
