<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.linepatrol.quest.module.QuestIssueGradeItem"  table="QUEST_ISSUE_GRADEITEM">
        <id name="id" type="string" unsaved-value="null" >
            <column name="id" sql-type="VARCHAR(12)" not-null="true"/>  
            <generator class="com.cabletech.commons.hb.StrSequenceGenerator">
               <param name="sequence">SEQ_QUEST_ISSUE_GRADEITEM_ID</param>
               <param name="length">12</param>
            </generator>
        </id>
        <property name="itemId" column="ITEM_ID"/>
        <property name="questId" column="QUESTIONNAIRE_ID"/>
    </class>
</hibernate-mapping>