<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.baseinfo.domainobjects.TempPoint"  table="temppointinfo">
        <id name="pointID" type="string" unsaved-value="null" >
            <column name="PointID" sql-type="char(32)" not-null="true"/>
            <generator class="assigned"/>
        </id>
         <property name="regionID"/>
        <property name="gpsCoordinate"/>
        <property name="simID"/>
        <property name="bedited"/>
        <property name="receiveTime"/>
    </class>
</hibernate-mapping>