<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.troublemanage.domainobjects.Accident"  table="accident">
        <!-- A 32 hex character is our surrogate key. It's automatically
            generated by Hibernate with the UUID pattern. -->
        <id name="keyid" type="string" unsaved-value="null" >
            <column name="keyid" sql-type="char(32)" not-null="true"/>
            <generator class="assigned"/>
        </id>
		<property name="patrolid"/>
		<property name="simid"/>
		<property name="sendtime"/>
		<property name="optype"/>
		<property name="gpscoordinate"/>
		<property name="pid"/>
		<property name="lid"/>
		<property name="operationcode"/>
		<property name="befortime"/>
		<property name="noticetime"/>
		<property name="cooperateman"/>
		<property name="testtime"/>
		<property name="testman"/>
		<property name="distance"/>
		<property name="realdistance"/>
		<property name="fixedman"/>
		<property name="monitor"/>
		<property name="commander"/>
		<property name="resonandfix"/>
		<property name="breportman"/>
		<property name="bconfirmman"/>
		<property name="regionid"/>
		<property name="isallblock"/>
		<property name="status"/>
		<property name="year"/>
		<property name="month"/>
        <property name="datumid"/>
    </class>
</hibernate-mapping>
