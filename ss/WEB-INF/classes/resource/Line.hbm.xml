<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.baseinfo.domainobjects.Line"  table="lineinfo">
        <id name="lineID" type="string" unsaved-value="null" >
            <column name="LineID" sql-type="char(32)" not-null="true"/>
            <generator class="assigned"/>
        </id>
        <property name="lineName"/>
        <property name="ruleDeptID"/>
        <property name="lineType"/>
        <property name="regionid"/>
        <property name="state"/>
		<property name="remark"/>
    </class>
</hibernate-mapping>

