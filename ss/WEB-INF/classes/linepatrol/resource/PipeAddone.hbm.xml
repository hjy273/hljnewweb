		<property name="threeLargeNumber" column="THREE_LARGE_NUMBER" />
		<property name="fourSmallNumber" column="FOUR_SMALL_NUMBER" />
		<property name="fourMiddleNumber" column="FOUR_MIDDLE_NUMBER" />
		<property name="fourLargelNumber" column="FOUR_LARGEL_NUMBER" />
		<property name="forntSmallNumber" column="FORNT_SMALL_NUMBER" />
		<property name="forntMiddleNumber" column="FORNT_MIDDLE_NUMBER" />
		<property name="forntLargeNumber" column="FORNT_LARGE_NUMBER" />
		<property name="otherSmallNumber" column="OTHER_SMALL_NUMBER" />
		<property name="otherMiddleNumber" column="OTHER_MIDDLE_NUMBER" />
		<property name="otherLargeNumber" column="OTHER_LARGE_NUMBER" />
