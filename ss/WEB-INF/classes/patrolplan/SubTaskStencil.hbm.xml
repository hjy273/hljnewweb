<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.planinfo.domainobjects.StencilSubTask"  table="subtask_stencil">
        <!-- A 32 hex character is our surrogate key. It's automatically
            generated by Hibernate with the UUID pattern. -->
        <id name="ID" type="string" unsaved-value="null" >
            <column name="taskid" not-null="true"/>
            <generator class="assigned"/>
        </id>
        <property name="stencilid"/>
        <property name="description"/>
        <property name="linelevel"/>
        <property name="excutetimes"/>
        <property name="evaluatepoint"/>
        <property name="factpointsnum"/>
    </class>
</hibernate-mapping>